{
    "version": 2,
    "builds": [
      {
        "src": "api/upload-profile-picture.mjs",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/upload-profile-picture.mjs"
      },
      {
        "src": "/(.*)",
        "dest": "/public/$1"
      }
    ]
  }